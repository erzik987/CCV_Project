@model IEnumerable<CCV_Project.Models.Rack>

@{
    ViewBag.Title = "Index";
}

@*<h2>Storehouse with id</h2>*@

@if (Session["StoreHouseId"] != null)
{
    <h4>
        Storehouse with id @Session["StoreHouseId"].ToString()
    </h4>
    <label>@Session["StoreHouseId"].ToString()</label>
}
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                    @Html.DisplayNameFor(model => model.StoreHouseRefId)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.EAN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Xposition)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Yposition)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Activ)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                        @Html.DisplayFor(modelItem => item.StoreHouseRefId)
                    </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EAN)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Xposition)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Yposition)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Activ)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.RackId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.RackId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.RackId })
                </td>
            </tr>
        }

    </table>
